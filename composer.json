  "description": "Artisan command for checking language resources.",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Karl Valentin",
      "email": "karl.valentin@kvis.de"
    }
  ],
  "require": {
    "php": "^7.4.0|^8.0|^8.1",
    "laravel/framework": "^7.0|^8.0|^9.1",
    "laravel/helpers": "^1.2"
